{
  "name": "Document Processing Pipeline",
  "nodes": [
    {
      "parameters": {
        "path": "doc-webhook",
        "httpMethod": "POST",
        "responseMode": "onReceived",
        "options": {
          "allowedOrigins": "*"
        }
      },
      "name": "Document Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [240, 300],
      "webhookId": "document-webhook-001"
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{$json.event}}",
              "operation": "equals",
              "value2": "file_added"
            }
          ]
        }
      },
      "name": "Check Event Type",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [440, 300]
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "status",
              "value": "received"
            },
            {
              "name": "event",
              "value": "={{$json.event}}"
            },
            {
              "name": "file_path",
              "value": "={{$json.file_path}}"
            },
            {
              "name": "timestamp",
              "value": "={{new Date().toISOString()}}"
            }
          ]
        }
      },
      "name": "Format Response",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [640, 200]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://search_api:8001/webhook/process",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "event",
              "value": "={{$json.event}}"
            },
            {
              "name": "file_path",
              "value": "={{$json.file_path}}"
            },
            {
              "name": "source",
              "value": "n8n_webhook"
            }
          ]
        },
        "options": {
          "timeout": 30000
        }
      },
      "name": "Forward to Search API",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [640, 400]
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "processing_status",
              "value": "queued"
            },
            {
              "name": "message",
              "value": "Document processing started"
            }
          ]
        }
      },
      "name": "Success Response",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [840, 300]
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "error",
              "value": "Event type not supported"
            },
            {
              "name": "received_event",
              "value": "={{$json.event}}"
            }
          ]
        }
      },
      "name": "Error Response",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [640, 600]
    }
  ],
  "connections": {
    "Document Webhook": {
      "main": [
        [
          {
            "node": "Check Event Type",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Check Event Type": {
      "main": [
        [
          {
            "node": "Format Response",
            "type": "main",
            "index": 0
          },
          {
            "node": "Forward to Search API",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Error Response",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Forward to Search API": {
      "main": [
        [
          {
            "node": "Success Response",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": true,
  "settings": {
    "saveManualExecutions": true,
    "callerPolicy": "workflowsFromSameOwner"
  },
  "versionId": "1",
  "meta": {
    "instanceId": "document-intelligence-system"
  }
}
